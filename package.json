{
  "name": "@pewresearch/block-library-primitives",
  "version": "1.0.0",
  "description": "A starting point for anyone looking to add functionality, control, and create your own custom block library using (mostly) core/blocks. These are primitive blocks, not style opinionated, and are meant to add functionality to existing blocks",
  "main": "index.js",
  "dependencies": {
    "@wordpress/scripts": "^23.7.1",
    "enquire.js": "^2.1.6"
  },
  "devDependencies": {
    "@pewresearch/eslint-config": "^1.0.7",
    "@pewresearch/prettier-config": "^1.0.0",
    "@wordpress/block-editor": "^9.7.0",
    "@wordpress/blocks": "^11.14.0",
    "@wordpress/components": "^19.17.0",
    "@wordpress/compose": "^5.13.0",
    "@wordpress/data": "^6.15.0",
    "@wordpress/edit-post": "^6.12.0",
    "@wordpress/edit-site": "^4.12.0",
    "@wordpress/element": "^4.13.0",
    "@wordpress/hooks": "^3.15.0",
    "@wordpress/i18n": "^4.15.0",
    "classnames": "^2.3.1",
    "underscore": "^1.13.4"
  },
  "prettier": "@pewresearch/prettier-config",
  "scripts": {
    "build": "wp-scripts build",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
